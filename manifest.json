{
    "manifest_version": 3,
    "name": "Mobility Quote Extractor",
    "description": "Extrai carros da página e insere em uma cotação formatada no modelo Primetour.",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  